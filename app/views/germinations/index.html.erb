<ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#generalView">General View</a></li>
    <li><a data-toggle="tab" href="#resultsGerminations">Results</a></li>
</ul>

<div class="tab-content">
    <div id="generalView" class="tab-pane fade in active">
        <br>
        <%= render 'optionsMenu', model: "Germination" %>
        <div class="panel panel-primary filterable">
            <div class="panel-heading">
                <h2 class="panel-title">Germinations</h2>
            </div>
            <br>
            <div class="table-responsive">
                <table class="table table-striped filterTable">
                    <button type="button" class="btn btn-primary clearFilters" aria-label="Left Align">
                        <span class="glyphicon glyphicon-refresh" aria-hidden="true"></span> Reset Filters
                    </button>
                    <br>
                    <br>
                    <thead>
                        <tr class="filters">
                            <th>Code Cross-#Repeat</th>
                            <th>Week</th>
                            <th>Father Code</th>
                            <th>Mother Code</th>
                            <th>Nro Germinations</th>
                            <!-- <th>Missing Seeds</th>                             -->
                            <th class="buttonAction"></th>
                            <th class="buttonAction"></th>
                        </tr>
                    </thead>
                    <tbody id="germinationsBody">
                        <% @germinations.each do |germination| %>
                        <%= render "germination", germination: germination %>
                        <% end %>
                    </tbody>
                </table>
            </div>
        </div>
        <%= render 'dialog' %>
    </div>

    <div id="resultsGerminations" class="tab-pane fade">
        <div class="panel panel-primary filterable">
            <div class="panel-heading">
                <h2 class="panel-title">Results by CodeCross</h2>
            </div>
            <br>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>CodeCross</th>
                            <th>Total</th>
                            <th>Percentaje</th>
                        </tr>
                    </thead>
                    <tbody>

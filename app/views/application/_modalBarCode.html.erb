<div class="text-center">
    <div class="modal fade" id="codeBar-modal-modal">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">Ã—</span><span
                            class="sr-only">Close</span></button>
                    <h4 class="modal-title">Create Bar Codes</h4>
                </div>
                <div>
                    <% case model %>
                    <% when "GeneticBank" %>
                    <%= form_tag(geneticBankBarCodes_path(format: "pdf"), method: :get) do %>                    
                    <%= select_tag "barCodeSelect", "",multiple: true%>
                    <%= submit_tag "Create Bar Codes", class: "btn btn-success"%>
                    <%end%>
                    <% when "Crossing" %>
                    <%= form_tag(crossingsBarCodes_path(format: "pdf"), method: :get) do %>                    
                    <%= select_tag "barCodeSelect","",multiple: true %>
                    <%= submit_tag "Create Bar Codes", class: "btn btn-success"%>
                    <%end%>    
                    <% when "Seed" %>
                    <%= form_tag(seedsBarCodes_path(format: "pdf"), method: :get) do %>                    
                    <%= select_tag "barCodeSelect","",multiple: true %>
                    <%= submit_tag "Create Bar Codes", class: "btn btn-success"%>
                    <%end%>    
                    <% when "IrbSelection" %>
                    <%= form_tag(irbSelectionsBarCodes_path(format: "pdf"), method: :get) do %>                                         
                     <%= select_tag "barCodeSelect","",multiple: true %>
                    <%= submit_tag "Create Bar Codes", class: "btn btn-success"%>
                    <%end%>  
                    <% when "SpekSelection" %>
                    <%= form_tag(spekSelectionsBarCodes_path(format: "pdf"), method: :get) do %>                    
                    <%= select_tag "barCodeSelect","",multiple: true %>
                    <%= submit_tag "Create Bar Codes", class: "btn btn-success"%>
                    <%end%> 
                    <% when "ConectiflorSelection" %>
                    <%= form_tag(conectiflorSelectionsBarCodes_path(format: "pdf"), method: :get) do %>                    
                    <%= select_tag "barCodeSelect","",multiple: true %>
                    <%= submit_tag "Create Bar Codes", class: "btn btn-success"%>
                    <%end%> 
                    <%end%>
                </div>
            </div>
        </div>
    </div>
</div>
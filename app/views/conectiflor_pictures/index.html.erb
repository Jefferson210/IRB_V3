<p id="notice"><%= notice %></p>

<h1>Listing Conectiflor Pictures</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conectiflor_pictures.each do |conectiflor_picture| %>
      <tr>
        <td><%= link_to 'Show', conectiflor_picture %></td>
        <td><%= link_to 'Edit', edit_conectiflor_picture_path(conectiflor_picture) %></td>
        <td><%= link_to 'Destroy', conectiflor_picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

